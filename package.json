{
  "name": "timrjs",
  "version": "1.1.0",
  "description": "Timr is a simple utility for creating timers in JavaScript.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf dist/ && rimraf lib/",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' src test",
    "test": "mocha --timeout 5000 --compilers js:babel-register",
    "test:watch": "yarn test -- --watch",
    "coverage": "babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --timeout 5000 --reporter spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "check:src": "yarn run lint && yarn run test",
    "build:commonjs": "NODE_ENV=development babel src --out-dir lib",
    "build:umd": "webpack --env.development --progress --colors",
    "build:umd:min": "webpack --env.production --progress --colors",
    "build": "yarn run build:commonjs && yarn run build:umd && yarn run build:umd:min",
    "prepublish": "yarn run clean && yarn run check:src && yarn run build"
  },
  "keywords": [
    "timr",
    "timer",
    "timrjs",
    "utility",
    "countdown",
    "stopwatch",
    "date"
  ],
  "bugs": {
    "url": "https://github.com/joesmith100/timrjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joesmith100/timrjs.git"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.0.0",
    "babel-register": "^6.24.1",
    "chai": "^4.0.1",
    "coveralls": "^2.13.1",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.3.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.2",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "object-assign": "^4.1.1",
    "webpack-config-utils": "^2.3.0"
  }
}
